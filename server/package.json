{
  "name": "pern-auth-server",
  "version": "1.0.0",
  "description": "Production-ready PERN authentication server with JWT and Google OAuth",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js --exec ts-node --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "ts-node src/db/migrate.ts up",
    "migrate:down": "ts-node src/db/migrate.ts down",
    "seed": "ts-node src/db/seed.ts"
  },
  "keywords": [
    "auth",
    "jwt",
    "oauth",
    "pern",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.3",
    "argon2": "^0.31.2",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "openid-client": "^5.6.4",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
